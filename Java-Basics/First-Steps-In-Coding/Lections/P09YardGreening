import java.util.Scanner;

public class YardGreening {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        double squareMeters = Double.parseDouble(scanner.nextLine());
        double totalCost = squareMeters * 7.61;
        double discount = totalCost * 0.18;
        double finalPrice = totalCost - discount;
        System.out.printf("The final price is: %f lv. %n", finalPrice);
        System.out.printf("The discount is: %f lv. %n", discount);
    }
}
